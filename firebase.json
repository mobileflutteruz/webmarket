{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "users": {
      "$uid": {
        ".read": "auth.uid == $uid",
        ".write": "auth.uid == $uid"
      }
    }
  },
  "database": {
    "rules": {
      ".read": "auth != null",
      ".write": "auth != null",
      "users": {
        "$uid": {
          ".read": "auth.uid == $uid",
          ".write": "auth.uid == $uid"
        }
      }
    }
  },
  "firestore": {
    "rules": "firebase.ruled",
    "indexes": "firebase.indexes.json"
  },
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "storage": {
    "rules": "storage.rules"
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "webmarket-6e86f",
          "appId": "1:650823189030:android:c614af7997b410093d9ce7",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "webmarket-6e86f",
          "configurations": {
            "android": "1:650823189030:android:c614af7997b410093d9ce7",
            "ios": "1:650823189030:ios:686a8ccf50cb01933d9ce7",
            "macos": "1:650823189030:ios:686a8ccf50cb01933d9ce7",
            "web": "1:650823189030:web:3864a88775f412d53d9ce7",
            "windows": "1:650823189030:web:9e5018c21e522ebb3d9ce7"
          }
        }
      }
    }
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5000
    },
    "pubsub": {
      "port": 8085
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "remoteconfig": {
    "template": " continue"
  }
}
